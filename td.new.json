{
  "family": "ace-worker",
  "taskRoleArn": "arn:aws:iam::049930841128:role/ace-worker-task-role",
  "executionRoleArn": "arn:aws:iam::049930841128:role/ace-worker-exec-role",
  "networkMode": "awsvpc",
  "cpu": "512",
  "memory": "2048",
  "containerDefinitions": [
    {
      "name": "ace-worker",
      "image": "049930841128.dkr.ecr.eu-north-1.amazonaws.com/ace-worker:latest",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "name": "ace-worker-80-tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "AWS_REGION",
          "value": "eu-north-1"
        },
        {
          "name": "AWS_S3_BUCKET",
          "value": "vercelcontentenginebucket"
        },
        {
          "name": "AWS_SQS_QUEUE_URL",
          "value": "https://sqs.eu-north-1.amazonaws.com/049930841128/video-jobs"
        },
        {
          "name": "DATABASE_URL",
          "value": "postgres://neondb_owner:npg_JjFn8eR3VKxr@ep-royal-dream-adacvf5w-pooler.c-2.us-east-1.aws.neon.tech/neondb?sslmode=require"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "eu-north-1",
          "awslogs-group": "/ecs/ace-worker",
          "awslogs-stream-prefix": "ace-worker"
        }
      },
      "systemControls": []
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ]
}
